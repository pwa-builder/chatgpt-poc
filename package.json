    "node-fetch": "^3.3.0",
    "openai": "^3.1.0",
    "puppeteer": "^19.5.2",
    "vite": "^4.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  }
